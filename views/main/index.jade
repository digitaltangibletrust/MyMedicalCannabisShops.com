extends /views/layout

block content
  +subscribePrompt

  .section-padding
    .container
      .row
        .col-md-3
          h3.no-margin-top Stores
          .row
            each partner in offerData.partners
              .col-xs-6
                +partnerBox(partner)

        .col-md-9
          h3.no-margin-top.hidden-md.hidden-lg Offers
          .row
            each offer in offerData.offers 
             +showOffer(offer)